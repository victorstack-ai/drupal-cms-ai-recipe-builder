{
  "name": "victorstack-ai/drupal-cms-ai-recipe-builder",
  "description": "Recipe starter and blueprint helper for AI-driven Drupal CMS site building.",
  "type": "drupal-recipe",
  "license": "MIT",
  "require": {
    "php": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "VictorStackAi\\DrupalCmsAiRecipeBuilder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VictorStackAi\\DrupalCmsAiRecipeBuilder\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "squizlabs/php_codesniffer": "^3.9"
  },
  "scripts": {
    "lint": "phpcs --standard=phpcs.xml.dist src tests",
    "test": "phpunit"
  }
}
